{
  "name": "marqua",
  "author": "Ignatius Bagus",
  "description": "Augmented Markdown Compiler",
  "version": "0.3.0",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "tsc -w -d --outDir .",
    "lint": "run-p lint:**",
    "lint:code": "tsc --noEmit",
    "lint:style": "prettier -c src test",
    "lint:types": "touch index.d.ts && tsd",
    "test": "run-p test:**",
    "test:apps": "node --loader ts-node/esm test/apps/index.ts",
    "publish:clean": "git clean -dfx -e node_modules",
    "publish:build": "tsc -d --outDir .",
    "publish:format": "prettier -wu **/*.{d.ts,js}",
    "prepublishOnly": "run-s publish:**"
  },
  "prettier": "mauss/prettier.json",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/marqua.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/marqua/issues"
  },
  "keywords": [
    "markdown",
    "compiler",
    "parser"
  ],
  "dependencies": {
    "@ignatiusmb/aqua": "^20.3.3",
    "markdown-it": "^12.0.4",
    "mauss": "^0.1.4"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^14.14.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "ts-node": "^10.5.0",
    "tsd": "^0.14.0",
    "typescript": "^4.1.2"
  },
  "tsd": {
    "directory": "test/typings"
  }
}
