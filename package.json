{
  "name": "marqua",
  "author": "Ignatius Bagus",
  "description": "Node.js Aqua-flavored markdown file parser",
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "tsc -w --noEmit",
    "publish:clean": "git clean -dfx -e node_modules",
    "publish:build": "tsc -d --outDir .",
    "publish:format": "prettier -wu **/*.{d.ts,js}",
    "prepublishOnly": "run-s publish:**"
  },
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/marqua.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/marqua/issues"
  },
  "dependencies": {
    "@ignatiusmb/aqua": "^20.3.3",
    "markdown-it": "^12.0.4",
    "mauss": "^0.0.8"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^14.14.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
